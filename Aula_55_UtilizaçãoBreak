# include <stdio.h>

int main (void){
    int cont = 0;
    for(int i = 1; i <= 20000; ++i){
            if(i % 3 == 0 && i % 9 == 0){
                 printf("%i divisivel por 3 e 9.\n", i);
                 ++cont;
                 break;
            }else{
                  printf("%i nao e divisivel.\n", i);
            }
    }
    printf("Houve %i divisiveis entre 1 e 20.000.\n", cont);
    
    getchar();
    return 0;
}
