# include <stdio.h>
# include <stdlib.h>

void stringsIguais(char s1[], char s2[]);

int main (void){

	stringsIguais(s1, s2);
	
	system("pause");
	return 0;
}

void stringsIguais(char s1[], char s2[]){
	
	int i = 0;
	
	while(s1[i] == s2[i] && s1[i] != '\0' && s2[i] != '\0'){
		++i;
	}
	
	if(s1[i] == '\0' && s2[i] == '\0'){
		printf("sao iguais\n");
	}else{
		printf("nao sao iguais\n");
	}
}
