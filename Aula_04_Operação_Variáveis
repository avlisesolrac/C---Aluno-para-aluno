// Operação com variaveis

# include <stdio.h>

int main ()
{
	int minhaIdade = 22;
	int maeIdade = 40;
	int paiIdade = 45;
	int irmaoIdade = 15;
	int idadeTotal;
	
	idadeTotal = minhaIdade + maeIdade + paiIdade + irmaoIdade;
	//idadeTotal = minhaIdade + maeIdade + paiIdade + irmaoIdade + 100;
	//idadeTotal = minhaIdade + maeIdade + paiIdade + irmaoIdade - 100;
	//idadeTotal = minhaIdade * maeIdade / paiIdade + irmaoIdade;
	
	printf("A idade total eh: %i\n\n", idadeTotal);
	
	return 0;
}
