# include <stdio.h>
# include <stdlib.h>

int main (void) {
	
	int vetor[5];
	
	struct horario {
		int horas;
		int minutos;
		int segundos;
	};
	
	struct horario agora;
	
	agora.horas = 15;
	
	agora.minutos = 17;
	
	agora.segundos = 30;
	
	printf("%i:%i:%i\n\n", agora.horas, agora.minutos, agora.segundos);
	
	system("pause");
	return 0;
}
